["+"|"-"|"*"|"/"|"%"] { printf("it is valid\nit is a arithmetic operator:"); }
[">"|"<"|"!"|"="] { printf("\nit is valid\nit is a relational operator:"); }
["&"|"^"|"~"|"|"] { printf("\nit is valid\nit is a bitwise operator:"); }
.+ { printf("\nit is not a valid operator:"); }
